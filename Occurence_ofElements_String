package selenium;

public  class Occurence_ofElements  {
	
public static void main(String[] args) {
	
	String str = "success";
	int count = 0;
	char c = str.toLowerCase().charAt(0);
	
	for(int i = 0; i<str.length();i++) {
		
		if (c==str.charAt(i)) {
			count ++;
			
		}
	
	}
	
	System.out.println(c + " occurs " + count + " times in "+ str);
	
	
	}
}
