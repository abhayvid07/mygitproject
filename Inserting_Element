package selenium;

public  class Inserting_Element  {
	
public static void main(String[] args) {
	
	int arr[]= { 10,20,30,60,20};
	int a = 100;
	int pos=3;
	
	for(int i=arr.length-1; i>pos-1;i--) {
		 
		arr[i]=arr[i-1];
		
	}
	
	arr[pos-1] = a;
	
	for(int i = 0; i<arr.length;i++) {
		
		System.out.println(arr[i] + " ");
		
		}
	}
}
